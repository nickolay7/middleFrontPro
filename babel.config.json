{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        [
            "i18next-extract",
            {
                "locales": ["en", "ru"],
                "keyAsDefaultValue": true,
                "saveMissing": true,
                "outputPath": "public/locales/{{locale}}/{{ns}}.json"
            }
        ]
    ]
}
