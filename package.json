{
    "name": "middle_front_project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:ui": "loki test",
        "start": "webpack serve",
        "start:vite": "vite",
        "start:dev": "concurrently \"npm run dev-server\" \"npm run start\"",
        "start:dev:vite": "concurrently \"npm run dev-server\" \"npm run start:vite\"",
        "build:prod": "webpack --env mode=production apiUrl=\"https://blog-server-kappa.vercel.app\"",
        "build:dev": "webpack --env mode=development",
        "component": "generate-react component",
        "lint:ts": " eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": " eslint \"**/*.{ts,tsx}\" --fix",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "storybook:build": "build-storybook -c ./config/storybook",
        "test:e2e": "cypress open",
        "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
        "test:ui:ok": "loki approve",
        "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "dev-server": "node json-server --watch json-server/db.json",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "updateImports": "node ./scripts/refactoring/updateImports.mjs",
        "createPApiForSharedUi": "node ./scripts/refactoring/updateImports.mjs",
        "postinstall": "node ./scripts/postinstall.js",
        "prettier": "prettier --write **/*.{ts,tsx,json}",
        "remove-feature": "ts-node ./scripts/remove-feature.ts"
    },
